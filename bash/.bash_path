# Cargo
export PATH="$PATH:$HOME/.cargo/bin"

# Go
export PATH="$PATH:$GOPATH/bin"

# nvm
if [ -s "$NVM_DIR/nvm.sh" ]; then
  . "$NVM_DIR/nvm.sh"
fi

# pyenv
if which pyenv > /dev/null; then
  eval "$(pyenv init -)";
fi

# rbenv
if which rbenv > /dev/null; then
  eval "$(rbenv init -)";
fi

# Yarn
export PATH="$PATH:$HOME/.yarn/bin"
